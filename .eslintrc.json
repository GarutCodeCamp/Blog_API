{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent":"off",
        "linebreak-style":"off",
        "no-console":"off",
        "consistent-return":"off",
        "no-underscore-dangle":"off",
        "no-unused-vars":"off"
    }
}
